              <stringProp name="BeanShellSampler.query">import java.sql.*;
import java.util.*;
import java.lang.*;
import org.apache.regexp.*;

//校验数据库
String driver=&quot;com.mysql.jdbc.Driver&quot;;
String url=&quot;jdbc:mysql://test.kuaihuoyun.com/kd_caesar_109?useUnicode=true&amp;characterEncoding=utf-8&quot;;
String username=&quot;root&quot;;
String password=&quot;pass321&quot;;
String querySql=&quot;SELECT * from t_finance_fund where  apply_station_id=&apos;&quot;+&quot;${__P(oidValue)}&quot;+&quot;&apos; and left(created,10)&gt;=DATE_SUB(CURRENT_DATE,INTERVAL 30 day)&quot; ;
log.info(&quot;数据库校验-上缴打款，querySql=&quot;+querySql);
Connection conn =null;
Class.forName(driver);
conn = (Connection)DriverManager.getConnection(url,username,password);

if(!conn.isClosed()){
	Statement statement=conn.createStatement();
	ResultSet rs = statement.executeQuery(querySql);
	rs.last();
	int resultCount =rs.getRow();
	log.info(&quot;数据库校验-上缴打款，查询条数：&quot;+resultCount);
	if(resultCount&gt;0){
		Failure=false;
		FailureMessage =&quot;上缴打款查询数据库成功，有数据可以查询&quot;;
	}
	else{
		Failure=true;
		FailureMessage =&quot;上缴打款查询数据库 无数据可查询或查询失败!&quot;;
	}

	 rs.close();
	 statement.close();
	 conn.close();
}else{
	Failure=true;
	FailureMessage =&quot;连接数据库失败或者数据库内内有历史数据!&quot;;
}</stringProp>
