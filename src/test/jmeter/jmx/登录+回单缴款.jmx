	else{
		Failure=true;
		FailureMessage =&quot;回单缴款失败&quot;;
		log.info(&quot;回单运单缴款失败！！！&quot;);
	}

	 rs.close();
	 statement.close();
	 conn.close();
}else{
	Failure=true;
	FailureMessage =&quot;连接数据库失败或者数据库内内有历史数据!&quot;;
}</stringProp>
